@article{Lars-ErikCederman08012001,
	Abstract = {Computational modeling is used to improve our understanding of how  the democratic peace unfolds as a historical process in time and  space. Whereas most of the conventional literature interprets the  phenomenon as a constant and universal law operating at the state  level, the author follows Immanuel Kant and treats democratic cooperation  as an emergent macroprocess. The current study explores three causal  mechanisms. First, strategic tagging introduces a way for democracies  to select out like-minded cooperators. Second, regime-sensitive alliances  enable democratic states to defend the gains of cooperation. Third,  liberal collective security complements the liberal alignments. Based  on these processes, it is possible to "grow" cooperative outcomes  in an inhospitable geopolitical environment. Because tagging alone  is insufficient, alliances, and sometimes even collective security,  are necessary to produce perpetual peace. Such outcomes are characterized  by high levels of spatial clustering.},
	Author = {Cederman, Lars-Erik},
	Date-Added = {2010-02-08 12:26:05 -0500},
	Date-Modified = {2010-02-08 12:26:05 -0500},
	Doi = {10.1177/0022002701045004004},
	Eprint = {http://jcr.sagepub.com/cgi/reprint/45/4/470.pdf},
	Journal = {Journal of Conflict Resolution},
	Keywords = {simulation involving space},
	Number = 4,
	Pages = {470-502},
	Title = {{Modeling the Democratic Peace as a Kantian Selection Process}},
	Volume = 45,
	Year = 2001,
	Bdsk-Url-1 = {http://dx.doi.org/10.1177/0022002701045004004}}

